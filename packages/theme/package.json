{
  "name": "@shellraining/theme",
  "version": "1.0.1",
  "author": "shellRaining",
  "main": "./src/index.ts",
  "dependencies": {
    "dayjs": "^1.11.13",
    "feed": "^5.1.0",
    "gray-matter": "^4.0.3",
    "mermaid": "^11.2.1",
    "markdown-it-mark": "^4.0.0",
    "markdown-it-mathjax3": "^4.3.2",
    "markdown-it-sub": "^2.0.0",
    "markdown-it-sup": "^2.0.0",
    "markdown-it-task-lists": "^2.1.1",
    "subset-font": "^2.4.0",
    "viewerjs": "^1.11.7"
  },
  "peerDependencies": {
    "@vueuse/core": "^10.0.0",
    "vitepress": "^2.0.0-alpha.12",
    "vue": "^3.0.0"
  },
  "exports": {
    ".": "./src/index.ts",
    "./config": "./dist/node/config.js"
  },
  "description": "shellRaining's blog theme",
  "files": [
    "dist",
    "src",
    "README.md",
    "tsconfig.json"
  ],
  "keywords": [
    "vitepress",
    "theme",
    "blog"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rm -rf ./dist/ && NODE_ENV=production rolldown -c ./rolldown.config.ts",
    "build:watch": "rolldown -c ./rolldown.config.ts -w",
    "dev": "NODE_ENV=development rolldown -c ./rolldown.config.ts -w",
    "test": "vitest",
    "format": "prettier --write \"src/**/*.{ts,vue,js,json}\""
  },
  "type": "module",
  "types": "./dist/node/config.d.ts",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.16.5",
    "@types/subset-font": "^2.3.0",
    "rolldown": "1.0.0-beta.21",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  }
}
